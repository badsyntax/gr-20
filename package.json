{
  "name": "gr-20",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "homepage": "https://badsyntax.github.io/gr-20/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/*.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:prettier": "prettier --check \"**/*.{json,svg,md,yml}\"",
    "lint:prettier:fix": "prettier --write '**/*.{json,svg,md,yml}'",
    "bundlewatch": "bundlewatch"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "ignorePatterns": "build",
    "extends": [
      "react-app",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/no-floating-promises": "error",
      "curly": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bundlewatch": {
    "files": [
      {
        "path": "build/static/js/runtime-main*.js",
        "maxSize": "2 kB"
      },
      {
        "path": "build/static/js/*.chunk.js",
        "maxSize": "118 kB"
      },
      {
        "path": "build/static/css/*.chunk.css",
        "maxSize": "25 kB"
      }
    ]
  },
  "reactSnap": {
    "skipThirdPartyRequests": true,
    "inlineCss": false,
    "minifyHtml": {
      "collapseWhitespace": false
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.svg",
        "options": {
          "parser": "html"
        }
      }
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(ol-?))"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.11",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.5",
    "@types/ol": "^6.4.1",
    "@types/react": "^16.9.54",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/reactstrap": "^8.5.2",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "bootstrap": "^4.5.3",
    "bundlewatch": "^0.3.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "file-saver": "^2.0.2",
    "jest-canvas-mock": "^2.3.0",
    "jspdf": "^2.1.1",
    "jszip": "^3.5.0",
    "node-sass": "^4.14.1",
    "ol": "^6.4.3",
    "ol-hashed": "^2.0.0",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-icons": "^3.11.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-snap": "^1.23.0",
    "react-topbar-progress-indicator": "^4.1.0",
    "react-virtualized": "^9.22.2",
    "react-vis": "^1.11.7",
    "reactstrap": "^8.6.0",
    "redux-mock-store": "^1.5.4",
    "source-map-explorer": "^2.5.0",
    "typescript": "^4.0.5",
    "web-vitals": "^0.2.4"
  }
}
