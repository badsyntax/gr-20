{
  "name": "gr-20",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://badsyntax.github.io/gr-20/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/1.*",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:prettier": "prettier --check \"**/*.{json,svg,md,yml}\"",
    "lint:prettier:fix": "prettier --write '**/*.{json,svg,md,yml}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "skipThirdPartyRequests": true,
    "inlineCss": false,
    "minifyHtml": {
      "collapseWhitespace": false
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.svg",
        "options": {
          "parser": "html"
        }
      }
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@types/ol": "^6.4.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "file-saver": "^2.0.2",
    "gh-pages": "^3.1.0",
    "jspdf": "^2.1.1",
    "jszip": "^3.5.0",
    "node-sass": "^4.14.1",
    "ol": "^6.4.3",
    "ol-hashed": "^2.0.0",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-snap": "^1.23.0",
    "react-topbar-progress-indicator": "^4.1.0",
    "react-virtualized": "^9.22.2",
    "react-vis": "^1.11.7",
    "reactstrap": "^8.6.0",
    "source-map-explorer": "^2.5.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  }
}
